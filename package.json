{
  "name": "basicskillbackend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "start": "ts-node --project tsconfig.json src/index.ts",
    "dev": "nodemon --legacy-watch",
    "seed": "ts-node scripts/seed.ts",
    "docker-dev": "docker-compose -f docker-compose.base.yaml -f docker-compose.dev.yaml",
    "docker-prod": "docker-compose -f docker-compose.base.yaml -f docker-compose.prod.yaml",
    "docker-init": "npm run docker-dev exec node npx prisma migrate dev --name init",
    "docker-seed": "npm run docker-dev exec node npm run seed",
    "docker-init-and-seed": "npm run docker-init && npm run docker-seed",
    "docker-test": "npm run docker-dev exec node npm run test",
    "clear-logs-windows": "rmdir logs /s /q && mkdir logs",
    "clear-logs-linux": "rm -rf logs && mkdir logs",
    "docker-migrate": "npm run docker-dev exec node npx prisma migrate",
    "docker-migrate-reset": "npm run docker-dev exec node npx prisma migrate reset",
    "docker-migrate-resolve": "npm run docker-dev exec node npx prisma migrate resolve",
    "generate-frontend": "ts-node scripts/generate-frontend.ts",
    "clean": "ts-node scripts/cleaner.ts",
    "docker-clean": "npm run docker-dev exec node npm run clean"
  },
